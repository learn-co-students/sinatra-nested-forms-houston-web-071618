<h1>Display your Pirate here</h1>

  <ul>
    <li><%= @pirates.name %></li>
    <li><%= @pirates.weight %></li>
    <li><%= @pirates.height %></li>
  </ul>

<h2>Display your ships here</h2>
  <ul>
    <%@ships.each do |ship|%>
    <li><%=ship.name%></li>
    <li><%=ship.type%></li>
    <li><%=ship.booty%></li>
    <%end%>
  </ul>
